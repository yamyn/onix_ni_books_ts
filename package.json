{
    "name": "onix-ni-books",
    "version": "0.0.1",
    "main": "./src/server/index.js",
    "scripts": {
        "build": "tsc --skipLibCheck",
        "start": "tsc --skipLibCheck && node ./build/config/server/index.js",
        "ts:watch": "tsc -w",
        "migrate": "migrate-mongo -f ./migrations/migrate-mongo-config.js"
    },
    "dependencies": {
        "body-parser": "1.18.2",
        "compression": "1.7.1",
        "cookie-parser": "1.4.3",
        "cors": "2.8.5",
        "csvtojson": "2.0.10",
        "dotenv": "8.2.0",
        "express": "4.16.2",
        "helmet": "3.9.0",
        "migrate-mongo": "7.1.0",
        "mongodb": "3.5.5",
        "mongoose": "5.7.5",
        "ts-node": "8.8.1"
    },
    "devDependencies": {
        "@types/body-parser": "1.19.0",
        "@types/compression": "1.7.0",
        "@types/cookie-parser": "1.4.2",
        "@types/cors": "2.8.6",
        "@types/dotenv": "8.2.0",
        "@types/express": "4.17.3",
        "@types/helmet": "0.0.45",
        "@types/migrate-mongo": "7.0.0",
        "@types/mongodb": "3.5.4",
        "@types/mongoose": "5.7.8",
        "@types/node": "13.9.5",
        "cz-conventional-changelog": "3.1.0",
        "source-map-loader": "0.2.4",
        "tslint": "6.1.0",
        "tslint-config-airbnb": "5.11.2",
        "tslint-config-prettier": "1.18.0",
        "tslint-eslint-rules": "5.4.0",
        "typescript": "3.8.3"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
